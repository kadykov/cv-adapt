{
  "name": "cv-adapt-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate:types": "tsx scripts/generate-api-types.ts",
    "generate:handlers": "msw-auto-mock src/api/openapi.json -o src/mocks/generated",
    "generate:all": "npm run generate:types && npm run generate:handlers",
    "pretest": "npm run generate:all"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "jsdom": "^26.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^20.17.19",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "msw": "^2.0.0",
    "msw-auto-mock": "^0.25.0",
    "openapi-msw": "^1.1.0",
    "openapi-typescript": "^6.0.0",
    "openapi-zod-client": "^1.18.3",
    "postcss": "^8.5.2",
    "tailwindcss": "^4.0.6",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.34.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "msw-auto-mock": {
    "output": "src/mocks/generated",
    "maxArrayLength": 3,
    "validateResponses": true
  }
}
